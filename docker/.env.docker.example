# Docker environment variables
# Copy this file to docker/.env and set secure passwords
# Docker Compose automatically loads .env from the compose file directory
POSTGRES_USER=community_hub
POSTGRES_PASSWORD=CHANGEME
POSTGRES_DB=community_hub
REDIS_PASSWORD=CHANGEME

# Only used when xpack.security.enabled=true (staging/prod).
# In development, security is disabled so this value is ignored.
ELASTIC_PASSWORD=CHANGEME
